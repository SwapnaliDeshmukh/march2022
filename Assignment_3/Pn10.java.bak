class Pn10
{
	public static void main(String args[])
	{
		for(int i=1;i<=5;i++)
		{
			for(int k=5;k>=i;k--)
			{
              System.out.print(" ");
			}	
			for(char a=69;a>65;a--)
			{for(int j=1;j<=i;j++)
			{
				
				 System.out.print(" "+a);
			
			}}
			System.out.println();
			
		}
	}
}
	
		